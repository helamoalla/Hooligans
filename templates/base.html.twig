							<img src="{{  asset('LOGOKHADAMNI.png') }}" class="h-10 mr-2" alt="Khadamni Logo" />
							<span
									class="self-center text-1.5xl text-white font-semibold whitespace-nowrap ">KHADAMNI™</span>
						</a>
					</div>
					<div class="grid grid-cols-2 gap-8 sm:gap-6 sm:grid-cols-3">
						<div>
							<h2 class="mb-6 text-sm font-bold text-gray-300 uppercase ">Resources</h2>
							<ul class="text-white ">
								<li class="mb-4">
									<a href="#" class="hover:underline">Emplois</a>
								</li>
								<li>
									<a href="#" class="hover:underline">Stages</a>
								</li>
							</ul>
						</div>
						<div>
							<h2 class="mb-6 text-sm font-bold text-gray-300 uppercase ">Follow us</h2>
							<ul class="text-white ">
								<li class="mb-4">
									<a href="#" class="hover:underline ">Github</a>
								</li>
								<li>
									<a href="#" class="hover:underline">Discord</a>
								</li>
							</ul>
						</div>
						<div>
							<h2 class="mb-6 text-sm font-bold text-gray-300 uppercase ">About</h2>
							<ul class="text-white ">
								<li class="mb-4">
									<a href="#" class="hover:underline">Privacy Policy</a>
								</li>
								<li>
									<a href="#" class="hover:underline">Terms &amp; Conditions</a>
								</li>
							</ul>
						</div>
					</div>
				</div>
				<hr class="my-6 border-gray-200 sm:mx-auto " />
