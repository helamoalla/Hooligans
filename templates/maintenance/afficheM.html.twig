{% extends 'index.html.twig' %}
{% block body %}


<body>
    

            <!-- Ticket Section Start -->
            <div class="page-body">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card card-table">
                                <!-- Table Start -->
                                <div class="card-body">
                                    <div class="title-header option-title">
                                        <h5>MAINTENANCE</h5>
                                    </div>
                                    <div>
                                        <div class="table-responsive">
                                            <table class="table ticket-table all-package theme-table" id="table_id">
                                                <thead>
                                                    <tr>
                                                        <th>
                                                            <div >
                                                                
                                                                <span>Id USER</span>
                                                            </div>
                                                        </th>
                                                        <th>
                                                            <span>Date</span>
                                                        </th>
                                                        <th>
                                                            <span>Pannes</span>
                                                        </th>
                                                        <th>
                                                            <span>Options</span>
                                                        </th>
                                                    </tr>
                                                </thead>

                                                <tbody>
                                                  {% for i in m %}
                                                    <tr>
                                                        
                                                                <td>{{i.getUser.getIdUser}}</td>
                                                           
                                                        
                                                         <td>{{i.getDateMaintenance|date("m/d/Y")}}</td>

                                                         <td>{% if i.isPanneMoteur =='1' %}panne_moteur|{% endif %}
                                                         {% if i.isPompeAEau =='1' %}Pompe A Eau|{% endif %}
                                                          {% if i.isPatin =='1' %}Patin|{% endif %}
                                                           {% if i.isEssuieGlace =='1' %}Essuie Glace|{% endif %}
                                                            {% if i.isRadiateur =='1' %}Radiateur|{% endif %}
                                                             {% if i.isVentilateur =='1' %}Ventilateur|{% endif %}
                                                              {% if i.isDuride =='1' %}Duride|{% endif %}
                                                               {% if i.isFuiteDHuile =='1' %}Fuite D'Huile|{% endif %}
                                                                {% if i.isVidange =='1' %}Vidange|{% endif %}
                                                                 {% if i.isFiltre =='1' %}Filtre|{% endif %}
                                                                  {% if i.isBatterie =='1' %}Batterie|{% endif %}
                                                                  {% if i.isAmortisseur =='1' %}Amortisseur|{% endif %}
                                                                  {% if i.isFreinMain =='1' %}Frein Main|{% endif %}
                                                                  {% if i.isFeuDEclairage =='1' %}Feu D'Eclairage|{% endif %}
                                                              
                                                         </td>

                                                      
                                                        <td>
                                                            <ul>
                                                               
                                                                <li>
                                                                   <a>
                                                                    <form action="{{path('supprimerM',{'id':i.getId}) }}"> <button><i class="ri-delete-bin-line"></i></button> </form>
                                                                 <!--<form action="{{path('modifierM',{'id':i.getId}) }}"> <button><i class="ri-delete-bin-line"></i></button> </form>-->
                                                                </a>
                                                                </li>
                                                            </ul>
                                                        </td>
                                                    </tr>
                                                 {% endfor %}
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <!-- Table End -->
                            </div>
                        </div>
                    </div>
                </div>

              
            </div>
            <!-- Ticket Section End -->
        </div>
        <!-- Page Body End-->

       
    </div>
    <!-- page-wrapper End-->

     {% endblock %}

   