{% extends 'index.html.twig' %}
{% block body %}
<!-- Container-fluid starts-->
            <div class="page-body">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="card card-table">
                                <div class="card-body">
                                    <div class="title-header option-title d-sm-flex d-block">
                                        <h5>Products List</h5>
                                        <div class="right-options">
                                            <ul>
                                                <li>
                                                    <a href="javascript:void(0)">import</a>
                                                </li>
                                                <li>
                                                    <a href="javascript:void(0)">Export</a>
                                                </li>
                                                <li>
                                                    <a class="btn btn-solid" href="ajouterGC">Add Product</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div>
                                        <div class="table-responsive">
                                            <table class="table all-package theme-table table-product" id="table_id">
                                                <thead>
                                                    <tr>
                                                        <th>GARAGE Image</th>
                                                        <th>GARAGE Name</th>
                                                        <th>Adresse</th>
                                                        <th>Numero</th>
                                                        <th>Panne moteur</th>
                                                        <th>Pompe A Eau</th>
                                                            <th>patin</th>
            <th>essuie_glace</th>
            <th>radiateur</th>
            <th>ventilateur</th>
            <th>duride</th>
            <th>fuite_d_huile</th>
            <th>vidange</th>
            <th>filtre</th>
            <th>batterie</th>
            <th>amortisseur</th>
            <th>frein_main</th>
           <th>feu_d_eclairage</th>
           <th>taux_de_reduction</th>
                                                        <th>Option </th>
                                                    </tr>
                                                </thead>

                                                <tbody>
                                                  {% for i in g %}
                                                    <tr>
                                                   
                                                  
                                                       <td>
                                                            <div class="table-image">
                                                                <img src="http://localhost/images/{{i.getImage}}" class="img-fluid" alt="">
                                                                   
                                                            </div>
                                                        </td>
                                                     
                                                        

        <!--<td>{{i.getImage}}</td>-->
         <td>{{i.getNomGarage}}</td>
            <td>{{i.getAdresse}}</td>
            <td>{{i.getNumero}}</td>
            <td>
                {{i.getPanneMoteur}}
            </td>
            <td>
                {{i.getPompeAEau}}
            </td>
            <td>{{i.getPatin}}</td>
            <td>{{i.getEssuieGlace}}</td>
            <td>{{i.getRadiateur}}</td>
            <td>
                {{i.getVentilateur}}
            </td>
            <td>
                {{i.getDuride}}
            </td>
            <td>
                {{i.getFuiteDHuile}}
            </td>
            <td>{{i.getVidange}}</td>
            <td>{{i.getFiltre}}</td>
            <td>{{i.getBatterie}}</td>
            <td>
                {{i.getAmortisseur}}
            </td>
            <td>
                {{i.getFreinMain}}
            </td>
          <td>
                {{i.getFeuDEclairage}}
            </td>
            <td>{{i.getTauxDeReduction}}</td>
            
       
   
 

                                                        <td>
                                                            <ul>
                                                                <li>
                                                                    <a >
                                                                     <form action="{{path('detailGC',{'id':i.getId}) }}"><button> <i class="ri-eye-line"></i></button>  </form>
                                                                    </a>
                                                                </li>

                                                                <li>
                                                                    <a>
                                                                        
                                                                        <form action="{{path('modifierGC',{'id':i.getId}) }}"> <button><i class="ri-pencil-line"></i></button> </form>
                                                                    </a>
                                                                </li>

                                                                <li>
                                                                  <a>
                                                                    <form action="{{path('supprimerGC',{'id':i.getId}) }}"> <button><i class="ri-delete-bin-line"></i></button> </form>
                                                                </a>
                                                                </li>
                                                            </ul>
                                                        </td>
                                                    </tr>



<div class="modal fade theme-modal remove-coupon" id="exampleModalToggle" aria-hidden="true" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header d-block text-center">
                    <h5 class="modal-title w-100" id="exampleModalLabel22">Are You Sure ?</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                        <i class="fas fa-times"></i>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="remove-box">
                        <p>The permission for the use/group, preview is inherited from the object, object will create a
                            new permission for this object</p>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-animation btn-md fw-bold" data-bs-dismiss="modal">No</button>
                    <button type="button" class="btn btn-animation btn-md fw-bold" data-bs-target="#exampleModalToggle2"
                        data-bs-toggle="modal" data-bs-dismiss="modal">Yes</button>
                        
                </div>
            </div>
        </div>
    </div>
    
    <div class="modal fade theme-modal remove-coupon" id="exampleModalToggle2" aria-hidden="true" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title text-center" id="exampleModalLabel12">Done!</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                        <i class="fas fa-times"></i>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="remove-box text-center">
                        <div class="wrapper">
                            <svg class="checkmark" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 52 52">
                                <circle class="checkmark__circle" cx="26" cy="26" r="25" fill="none" />
                                <path class="checkmark__check" fill="none" d="M14.1 27.2l7.1 7.2 16.7-16.8" />
                            </svg>
                        </div>
                        <h4 class="text-content">It's Removed.</h4>
                    </div>
                </div>
                <div class="modal-footer">
                  <button class="btn btn-primary" data-bs-toggle="modal" data-bs-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>

                                                   
                                                 {% endfor %}
                                                    
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Container-fluid Ends-->
                {% endblock %}