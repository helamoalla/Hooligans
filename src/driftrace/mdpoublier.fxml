<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="driftrace.MdpoublierController">
   <children>
      <ImageView fitHeight="417.0" fitWidth="654.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../Downloads/Untitled%20design%20(1).png" />
         </image>
      </ImageView>
      <Label layoutX="143.0" layoutY="201.0" text="Entre votre E-mail" textFill="WHITE" />
