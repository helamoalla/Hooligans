      <Button fx:id="send" layoutX="307.0" layoutY="241.0" mnemonicParsing="false" onAction="#sendb" text="send" />
      <Button fx:id="ret" layoutX="591.0" layoutY="25.0" mnemonicParsing="false" onAction="#retour" text="Back" textFill="RED">
